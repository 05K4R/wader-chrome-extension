{
	"manifest_version": 2,

	"name": "Wader",
	"description": "Categorize and label tracks posted or reposted by profiles you follow on SoundCloud to determine if they are worth to keep following.",
	"version": "0.0.1",

	"browser_action": {
		"default_popup": "popup.html"
	},

	"background": {
		"page": "background.html"
	},

	"content_scripts": [
		{
			"matches": [ "*://soundcloud.com/stream"],
			"js": ["soundcloud/soundcloud_content.js"],
			"run_at": "document_end"
		}
	],

	"permissions": [
		"identity"
	],

	"content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",

	"oauth2": {
	  "client_id": "68541536183-ht44ou1s0ktae6hekmnv3njulssadk7g.apps.googleusercontent.com",
	  "scopes": [
		"https://www.googleapis.com/auth/userinfo.email",
		"https://www.googleapis.com/auth/userinfo.profile"
	  ]
	},
	"key": ""
}
